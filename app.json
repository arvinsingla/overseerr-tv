{
  "expo": {
    "scheme": "routertv",
		"userInterfaceStyle": "automatic",
    "plugins": [
      [
        "@react-native-tvos/config-tv",
        {
          "androidTVBanner": "./assets/tv_icons/seerrtv-icon-small-1x.png",
          "appleTVImages": {
            "icon": "./assets/tv_icons/seerrtv-icon.png",
            "iconSmall": "./assets/tv_icons/seerrtv-icon-small-1x.png",
            "iconSmall2x": "./assets/tv_icons/seerrtv-icon-small-2x.png",
            "topShelf": "./assets/tv_icons/seerrtv-shelf-1x.png",
            "topShelf2x": "./assets/tv_icons/seerrtv-shelf-2x.png",
            "topShelfWide": "./assets/tv_icons/icon-2320x720.png",
            "topShelfWide2x": "./assets/tv_icons/icon-4640x1440.png"
          }
        }
      ],
      "expo-router",
      "expo-font",
      "expo-build-properties",
      "expo-web-browser"
    ],
    "android": {
      "edgeToEdgeEnabled": true,
      "splash": {
        "image": "./assets/tv_icons/seerrtv-launch.png"
      }
    },
    "ios": {
      "splash": {
        "image": "./assets/tv_icons/seerrtv-launch.png"
      },
      "bundleIdentifier": "com.anonymous.overseerr-tv"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/favicon.png",
      "output": "static"
    },
    "experiments": {
      "typedRoutes": true
    },
    "newArchEnabled": true,
    "name": "seerr-tv",
    "slug": "seerr-tv"
  }
}
